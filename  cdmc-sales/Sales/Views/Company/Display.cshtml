@using Utl;
@model Entity.Company
<fieldset>
    <legend>@SR.Form</legend>

    <div class="display-label">公司名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ComapanyTitle)
    </div>

    <div class="display-label">联系方式</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Contact)
    </div>
    <div class="display-label">公司类型</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CompanyType.CompanyTypeName)
    </div>
    <div class="display-label">公司地址</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Address)
    </div>
    <div class="display-label">外资比例</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ForeignAssetPercentage)
    </div>
    <div class="display-label">内资比例</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DomesticAssetPercentage)
    </div>
</fieldset>
@Html.Partial("displaycommon", Model)

