@using Entity
@using Utl
@model List<ImportCompanyTrace>
@{ViewBag.Title = "历史导入操作";}
@Html.Partial("contenttitle", "历史导入操作")
@(Html.Telerik().Grid(Model).Name("Trace")
                .DataKeys(keys =>
                {
                    keys.Add(s => s.ID);
                })
                .Columns(c =>
                {
                    c.Bound(item => item.ImportUserName);
                    c.Bound(item => item.ImportDate);
                    c.Bound(item => item.ImportCompanyCount);
                    c.Bound(item => item.ImportLeadCount);
                    c.Bound(item => item.ImportTargetProject);
                    c.Bound(item => item.ImportSourceProject);
                })

                .Resizable(resizing => resizing.Columns(true))
                .Groupable()
                .Sortable()
                .Filterable()
                .Scrollable(scrolling => scrolling.Height(350))
                        )
