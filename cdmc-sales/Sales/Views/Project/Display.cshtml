@model Entity.Project
@using Utl

<fieldset>
    <legend>@SR.Form</legend>
    <div class="display-label">项目名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name )
    </div>

    <div class="display-label">激活</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsActived)
    </div>

     <div class="display-label">项目编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ProjectCode)
    </div>

    <div class="display-label">开始时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartDate)
    </div>

    <div class="display-label">结束时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EndDate)
    </div>

    <div class="display-label">销售目标</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Target)
    </div>

    <div class="display-label">销售简介</div>
    <div class="display-field">
        @HtmlExtensions.GetHtmlString(Model.SaleBrief)
    </div>
</fieldset>
