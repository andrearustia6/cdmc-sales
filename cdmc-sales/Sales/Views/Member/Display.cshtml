@model Entity.Member
@using Utl
@using Entity
<fieldset>
    <legend>@SR.Form</legend>

    <div class="display-label">Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">Characters</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Characters)
    </div>

     <div class="editor-field">
        @Html.HiddenFor(m=>m.ProjectID)
    </div>

    <div class="display-label">Project</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Project.Name)
    </div>

    <div class="display-label">参与过的项目</div>
    <div class="display-field">
    @{
        var ps = Model.GetInvolveProjects();
        if(ps.Count>0)
        {
            <table style="80%">
            <tr><th>项目名称</th><th>总CheckIn</th><th>总DealIn</th></tr>
            @foreach (var p in ps)
            {
               @* <tr><td>@p.Name</td><td>@p.ge</td><td></td></tr>*@
            }
            </table>
        }
     }
        @Html.DisplayFor(model => model.Project.Name)
    </div>


   
</fieldset>

