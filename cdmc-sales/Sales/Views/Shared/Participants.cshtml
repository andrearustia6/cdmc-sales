@using Entity
@using Utl
@model List<Participant>
<center>
<div >
@if (Model != null && Model.Count > 0)
{ 
 @(Html.Telerik().PanelBar().Name("p").Items(childs =>
{
    foreach (var p in Model)
    {
        Participant t = p;
        childs.Add().Text(p.Name + "  " + p.Title + "   " + p.Contact).Content(@<text>@RenderChild(t)</text>);
    }
})
)
}  
</div>
</center>

@helper RenderChild(Participant i)
{
    <div>
    @{
    @Html.Partial("ParticipantInfo", i);
        }
    </div>
}
